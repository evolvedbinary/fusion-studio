    "@theia/process": "^1.5.0",
    "@theia/terminal": "^1.5.0",
    "@theia/workspace": "^1.5.0",
    "fs-extra": "9.0.1",
    "fusion-studio-extension": "^1.0.0",
    "xterm": "^4.7.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@theia/cli": "^1.5.0",
    "electron-builder": "^22.7.0",
    "electron-builder-notarize": "^1.2.0"
  }
}
